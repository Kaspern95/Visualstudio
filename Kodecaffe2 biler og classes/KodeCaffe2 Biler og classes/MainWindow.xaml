<Window x:Class="KodeCaffe2_Biler_og_classes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KodeCaffe2_Biler_og_classes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="47*"/>
            <RowDefinition Height="372*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="65*"/>
            <ColumnDefinition Width="41*"/>
            <ColumnDefinition Width="92*"/>
        </Grid.ColumnDefinitions>
        <Label  Grid.Row="0" Grid.Column="0" Content="Biler i min beholdning" FontSize="25"></Label>
        <ListBox SelectionMode="Extended" Name="CarListBox" Grid.Column="0" Grid.Row="1"></ListBox>
        <Label Name="MyMoney" Grid.Row="0" Grid.Column="1" Content="Mine Penge: " FontSize="25" Grid.ColumnSpan="2"></Label>
        <Button Grid.Row="1" Grid.Column="1" Content="SELL CAR" FontSize="25" Margin="20,130" Click="btn_SellCar"></Button>
    </Grid>
</Window>
